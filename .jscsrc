{
  "preset": "ember-suave",
  "requireParenthesesAroundArrowParam": false,
  "requireArrayDestructuring": false,
  "requireCurlyBraces": [
    "else",
    "for",
    "while",
    "do",
    "try",
    "catch",
    "case",
    "default"
  ],
  "requireDotNotation": { "allExcept": [ "snake_case" ] }
}
